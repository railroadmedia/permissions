{
  "name": "railroad/permissions",
  "type": "library",
  "description": "Permissions service",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "laravel/framework": "^9.0",
    "guzzlehttp/guzzle": "^7.2",
    "railroad/resora": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.10",
    "phpunit/php-code-coverage": "^9.2.10",
    "orchestra/testbench": "^7.4",
    "dms/phpunit-arraysubset-asserts": "^0.4.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Railroad\\Permissions\\Providers\\PermissionsServiceProvider"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "Railroad\\Permissions\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Railroad\\Permissions\\Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "kylekatarnls/update-helper": true
    }
  }
}
