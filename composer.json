{
  "name": "railroad/permissions",
  "type": "library",
  "description": "Permissions service",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "require": {
    "php": "~7",
    "laravel/framework": "5.6.*",
    "guzzlehttp/guzzle": "~6.0",
    "railroad/resora": "1.0.*"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "phpunit/php-code-coverage": "6.0.1",
    "orchestra/testbench": "~3.6",
    "filp/whoops": "~2.1"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Railroad\\Permissions\\Providers\\PermissionsServiceProvider"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "Railroad\\Permissions\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Railroad\\Permissions\\Tests\\": "tests/"
    }
  }
}
